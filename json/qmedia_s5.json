		"attribute": [
          "//div[@class='stui-content__detail']/p[1]/allText()",
          "//div[@class='stui-content__detail']/p[2]/allText()",
          "//div[@class='stui-content__detail']/p[3]/allText()",
          "//div[@class='stui-content__detail']/p[4]/allText()"
