 {
	"root": "https://www.jiujiukp.com/",
	"search_format": "https://www.jiujiukp.com/search?wd=%s",
	"isPost": false,
	"search_rule": {
		"title": "//div[@class='module-search-item']/div[1]/div[1]/a/img/@alt",
        "link": "//div[@class='module-search-item']/div[1]/div[1]/a/@abs:href",
        "img": "//div[@class='module-search-item']/div[1]/div[1]/a/img/@abs:data-src"
	},
	"detail_rule": {
		"play_url": "//div[@class='online-button']/a/@abs:href",
        "intro": "//span[@class='sketch content']/allText()",
        "thumb": "//a[@class~='myui-vodlist__thumb']/img/@abs:data-original",
        "lines": "//div[@class='tab-content myui-panel_bd']/div",
        "items": "//div[@class='tab-content myui-panel_bd']/div/ul/li/a",
		"attribute": [
          "//div[@class='myui-content__detail']/p[1]/allText()",
          "//div[@class='myui-content__detail']/p[2]/allText()",
          "//div[@class='myui-content__detail']/p[3]/allText()",
          "//div[@class='myui-content__detail']/p[4]/allText()"
        ]
	}, 
    "isPageFinish":false,
	"play_video":""
} 