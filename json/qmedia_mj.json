 {
	"root": "https://www.meiju11.com/",
	"search_format": "https://www.meiju11.com/search.php?searchword=%s",
	"isPost": false,
	"search_rule": {
		"title": "//dl[@class='content']/dd/div[1]/h3/a/text()",
        "link": "//dl[@class='content']/dd/div[1]/h3/a/@abs:href",
        "img": "//dl[@class='content']/dt/a/@style"
	},
	"detail_rule": {
		"play_url": "",
        "intro": "//p[@class='detail-intro']/allText()",
        "thumb": "",
        "items": "//ul[@class='detail-play-list']/li/a",
		"attribute": [
          "//ul[@class='detail-actor']/li[1]/allText()",
          "//ul[@class='detail-actor']/li[2]/allText()",
          "//ul[@class='detail-actor']/li[3]/allText()",
          "//ul[@class='detail-actor']/li[4]/allText()"
        ]
	}, 
	"play_video":"strict"
} 