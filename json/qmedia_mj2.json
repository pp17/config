 {
	"root": "https://www.imeiju.io/",
	"search_format": "https://www.imeiju.io/?s=%s",
	"isPost": false,
	"search_rule": {
		"title": "//div[@class~='list-content']//article/a/h2/text()",
        "link": "//div[@class~='list-content']//article/a/@abs:href",
        "img": "//div[@class~='list-content']//article/a//img/@abs:data-original"
	},
	"detail_rule": {
		"play_url": "",
        "intro": "//p[@class='detail-intro']/allText()",
        "thumb": "",
        "items": "//ul[@class='detail-play-list']/li/a",
		"attribute": [
          "//ul[@class='detail-actor']/li[1]/allText()",
          "//ul[@class='detail-actor']/li[2]/allText()",
          "//ul[@class='detail-actor']/li[3]/allText()",
          "//ul[@class='detail-actor']/li[4]/allText()"
        ]
	}, 
	"play_video":"strict"
} 