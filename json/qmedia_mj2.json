 {
	"root": "https://www.imeiju.io/",
	"search_format": "https://www.imeiju.io/search.php?searchword=%s",
	"isPost": false,
	"search_rule": {
		"title": "//div[@class~='m-movies clearfix']/article/a/h2/text()",
        "link": "//div[@class~='m-movies clearfix']/article/a/@abs:href",
        "img": "//div[@class~='m-movies clearfix']/article/a/div/img/@abs:src"
	},
	"detail_rule": {
		"play_url": "",
        "intro": "//div[@class='abstract-content']/allText()",
        "thumb": "",
        "items": "//div[@id='video_list_li']/a",
		"attribute": [
          "//ul[@id='video_list']/li[1]/allText()",
          "//ul[@id='video_list']/li[2]/allText()",
          "//ul[@id='video_list']/li[5]/allText()",
          "//ul[@id='video_list']/li[8]/allText()"
        ]
	}, 
	"play_video":"strict"
} 