 {
	"root": "http://m.msong.net/",
	"search_format": "http://m.msong.net/tv-search-wd-%s.html",
	"isPost": false,
	"search_rule": {
		"title": "//ul[@id~='data_list']/li/div[1]/a/@title",
        "link": "//ul[@id~='data_list']/li/div[1]/a/@abs:href",
        "img": "//ul[@id~='data_list']/li/div[1]/a/img/@abs:data-src"
	},
	"detail_rule": {
		"play_url": "//div[@class='online-button']/a/@abs:href",
        "intro": "//div[@class~='tabConList']/div[2]/div/allText()",
        "thumb": "//div[@class='posterPic']/img/@src",
        "lines": "//div[@class~='yunbo_url']",
        "items": "//div[@class~='yunbo_url']/ul/li/a",
		"attribute": [
          "//p[@class='pBottom']/span[4]/allText()",
          "//p[@class='pBottom']/span[5]/allText()",
          "//p[@class='pBottom']/span[1]/allText()",
          "//p[@class='pBottom']/span[2]/allText()",
          "//p[@class='pBottom']/span[6]/allText()"
        ]
	}, 
    "isPageFinish":false,
	"play_video":""
} 