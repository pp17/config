{
	"root": "http://www.gulangyc.com/",
	"search_format": "http://www.gulangyc.com/search/%s.html",
	"isPost": false,
	"search_rule": {
		"title": "//ul[@id='searchList']/li/div[1]/a/img/@alt",
        "link": "//ul[@id='searchList']/li/div[1]/a/@abs:href",
        "img": "//ul[@id='searchList']/li/div[1]/a/img/@abs:data-original"
	},
	"detail_rule": {
		"play_url": "//div[@class='online-button']/a/@abs:href",
        "intro": "//p[@class='data hidden-xs']/text()",
        "thumb": "//a[@class='myui-vodlist__thumb picture']/img/@abs:data-original",
        "items": "//ul[@class='myui-content__list sort-list clearfix']/li/a",
		"attribute": [
          "//div[@class='myui-content__detail']/p[2]/allText()",
          "//div[@class='myui-content__detail']/p[3]/allText()",
          "//div[@class='myui-content__detail']/p[4]/allText()"
        ]
	},
	"play_video":""
}