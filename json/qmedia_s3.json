{
	"root": "https://www.freekan.cc/",
	"search_format": "https://www.freekan.cc/search.php?searchword=%s",
	"isPost": false,
	"search_rule": {
		"title": "//ul[@id='searchList']/li/div[1]/a/@title",
            "link": "//ul[@id='searchList']/li/div[1]/a/@abs:href",
            "img": "//ul[@id='searchList']/li/div[1]/a/@data-original"
	},
	"detail_rule": {
            "play_url": "//a[@class='ico play']/@abs:href",
            "intro": "//div[@class='tab-content myui-panel_bd']/allText()",
            "thumb": "//div[@class='img']/img/@src",
            "items": "//div[@class='tab-content myui-panel_bd']/div/ul/li/a",
			"attribute": ["//div[@class='myui-content__detail']/p[2]/allText()","//div[@class='myui-content__detail']/p[3]/allText()","//div[@class='myui-content__detail']/p[5]/allText()"]
	},
	"play_video":""
}