	"search_rule": {
		"title": "//ul[@id='searchList']/li/div[1]/a/@title",
            "link": "//ul[@id='searchList']/li/div[1]/a/@abs:href",
            "img": "//ul[@id='searchList']/li/div[1]/a/@data-original"
	},
	"detail_rule": {
		"play_url": "",
        "intro": "//span[@class='data']/allText()",
        "thumb": "",
        "lines": "//div[@class~='tab-pane fade in']",
        "items": "//div[@class~='tab-pane fade in']/ul[@class='stui-content__playlist clearfix']/li/a",
		"attribute": [
          "//div[@class='myui-content__detail']/p[1]/allText()",
          "//div[@class='myui-content__detail']/p[2]/allText()",
          "//div[@class='myui-content__detail']/p[3]/allText()",
          "//div[@class='myui-content__detail']/p[4]/allText()"
        ]
	}, 
    "isPageFinish":false,
	"play_video":""
} 