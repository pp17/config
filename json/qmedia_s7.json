 {
	"root": "http://www.unss.net/",
	"search_format": "http://www.unss.net/vodsearch/-------------.html?wd=%s",
	"isPost": false,
	"search_rule": {
		"title": "//ul[@class~='stui-vodlist__media']/li/div[1]/a/@title",
        "link": "//ul[@class~='stui-vodlist__media']/li/div[1]/a/@abs:href",
        "img": "//ul[@class~='stui-vodlist__media']/li/div[1]/a/@abs:data-original"
	},
	"detail_rule": {
		"play_url": "//a[@class='btn btn-primary']/@abs:href",
        "intro": "//p[@class='col-pd']/allText()",
        "thumb": "//a[@class~='myui-vodlist__thumb']/img/@abs:data-original",
        "lines": "//ul[@class='stui-content__playlist clearfix']/div",
        "items": "//ul[@class='stui-content__playlist clearfix']/li/a",
		"attribute": [
          "//div[@class='stui-content__detail']/p[1]/allText()",
          "//div[@class='stui-content__detail']/p[2]/allText()",
          "//div[@class='stui-content__detail']/p[3]/allText()",
          "//div[@class='stui-content__detail']/p[4]/allText()"
        ]
	}, 
    "isPageFinish":false,
	"play_video":""
} 