	"root": "https://9bo.tv/",
	"search_format": "https://9bo.tv/search?wd=%s",
	"isPost": true,
	"search_rule": {
		"title": "//div[@class='movie-item']/a/@title",
        "link": "//div[@class='movie-item']/a/@abs:href",
        "img": "//div[@class='movie-item']/a/img/@src"
	},
