 {
	"root": "https://www.bjoku.com/",
	"search_format": "https://www.bjoku.com/sosou.html?wd=%s&submit=",
	"isPost": false,
	"search_rule": {
		"title": "//ul[@class='stui-vodlist__media col-pd clearfix']/li/div[1]/a/@title",
        "link": "//ul[@class='stui-vodlist__media col-pd clearfix']/li/div[1]/a/@abs:href",
        "img": "//ul[@class='stui-vodlist__media col-pd clearfix']/li/div[1]/a/@abs:data-original"
	},
	"detail_rule": {
		"play_url": "",
        "intro": "//p[@class='desc detail hidden-xs']/allText()",
        "thumb": "",
        "lines": "//ul[@class='stui-content__playlist column8 clearfix']",
        "defLine": 1,
        "items": "//ul[@class='stui-content__playlist column8 clearfix']/li/a",
		"attribute": [
          "//div[@class='stui-content__detail']/p[1]/allText()",
          "//div[@class='stui-content__detail']/p[2]/allText()",
          "//div[@class='stui-content__detail']/p[3]/allText()",
          "//div[@class='stui-content__detail']/p[4]/allText()"
        ]
	}, 
    "isPageFinish":false,
	"play_video":""
} 